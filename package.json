{
  "name": "next-rock",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema=./src/prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:push": "prisma db push --schema=./src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "prisma:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.2",
    "@mui/material": "^6.1.2",
    "@mui/material-nextjs": "^6.1.2",
    "@prisma/client": "^6.1.0",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "next": "14.2.13",
    "prisma": "^6.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-use": "^17.6.0",
    "ts-node": "^10.9.2",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "typescript": "^5"
  }
}
